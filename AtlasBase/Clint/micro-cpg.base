            C@(c_)[@(ir_)] += pr[@(k_)];
            C@(c_)[@(ii_)] += pi[@(k_)];
@ROUT C2blk 
            pr[@(k_)] += C@(c_)[@(ir_)];
@ROUT blk2C C2blk
            @endiif
            @iif beta = -1
