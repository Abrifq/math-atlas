         @iwhile k < @(n)
            C@(k) += mr-nr;
            @iexp k @(k) 1 +
         @endiwhile
         @skip "for nr cleanup, it works like mu=nu"
         @callproc doDiBlock @(n)
      @endiif
   @endiif
      break;
      @iexp n @(n) 1 +
   @endiwhile
   default:;
   }
@endiif
@endiif
@iif na ! 1
@iif mu > 1
   rr = mnf % @(na);
   switch(mnr)
   {
@ROUT blk2C `      const @(typ) *p;`
@ROUT C2blk `      @(typ) *p;`
   @iexp n 1
   @iwhile n < @(mu)
   case @(n):
      p = b;
      @callproc doDirrBlockna @(n) @(mumu)
         break;
      @iexp n @(n) 1 +
   @endiwhile
   default:;
   }
@endiif
@endiif
